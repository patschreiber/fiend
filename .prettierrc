{
  "endOfLine": "lf",
  "trailingComma": "es5",
  "formatOnSave": false,
  "arrowParens": "always",
  "[javascript, typescript]": {
    "formatOnSave": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "parser": "typescript",
    "printWidth": 80,
    "quoteProps": "consistent",
    "proseWrap": "always"
  },
  "[json]": {
    "tabWidth": 4,
    "singleQuote": false
  }
}
