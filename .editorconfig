# top-most EditorConfig file
root = true

# Every .
# Unix-style newlines.
# Intent 2 spaces.
# UTF-8 encoding.
# Trims trailing whitespace.
# Newline ending every file.
[*]
end_of_line = LF
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
